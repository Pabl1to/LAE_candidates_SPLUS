SELECT det.Field, det.ra, det.dec, u.u_auto, u.e_u_auto, n1.J0378_auto, n1.e_J0378_auto, n2.J0395_auto, n2.e_J0395_auto, n3.J0410_auto, n3.e_J0410_auto, n4.J0430_auto, n4.e_J0430_auto, g.g_auto, g.e_g_auto, n5.J0515_auto, n5.e_J0515_auto, r.r_auto, r.e_r_auto, n6.J0660_auto, n6.e_J0660_auto, i.i_auto, i.e_i_auto, n7.J0861_auto, n7.e_J0861_auto, z.z_auto, z.e_z_auto, sgq.CLASS FROM idr3.detection_image as det JOIN idr3.u_band as u on det.ID = u.ID JOIN idr3.g_band as g on det.ID = g.ID JOIN idr3.r_band as r on det.ID = r.ID JOIN idr3.i_band as i on det.ID = i.ID JOIN idr3.z_band as z on det.ID = z.ID JOIN idr3.j0378_band as n1 on det.ID = n1.ID JOIN idr3.j0395_band as n2 on det.ID = n2.ID JOIN idr3.j0410_band as n3 on det.ID = n3.ID JOIN idr3.j0430_band as n4 on det.ID = n4.ID JOIN idr3.j0515_band as n5 on det.ID = n5.ID JOIN idr3.j0660_band as n6 on det.ID = n6.ID JOIN idr3.j0861_band as n7 on det.ID = n7.ID JOIN idr3_vacs.star_galaxy_quasar as sgq on det.ID = sgq.ID WHERE r.e_r_auto < 0.2 AND 1 = CONTAINS( POINT('ICRS', det.ra, det.dec), CIRCLE('ICRS', 352.103, -0.219, 4)) AND det.PhotoFlagDet = 0 and g.PhotoFlag_g = 0 and r.PhotoFlag_r = 0
